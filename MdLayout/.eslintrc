{
    "parser": "babel-eslint",
    "extends":
    [
        "eslint:recommended",
        "plugin:react/recommended",
        "google"
    ],
    "plugins":
    [
        "react"
    ],
    "parserOptions":
    {
        "ecmaVersion": 6,
        "sourceType": "module",
        "ecmaFeatures":
        {
            "impliedStrict": true,
            "jsx": true,
            "experimentalObjectRestSpread": true
        }
    },
    "globals":
    {
        "window": true,
        "module": true,
        "console": true,
        "document": true,
        "require": true
    },
    "env":
    {
        "es6": true
    },
    "rules":
    {
        "linebreak-style": "off",
        "no-console":
        [
            "error",
            {
                "allow":
                [
                    "warn",
                    "error",
                    "debug",
                    "info"
                ]
            }
        ],
        "no-debugger": "warn",
        "max-len": "off",
        "object-curly-spacing": "off"
    }
}
