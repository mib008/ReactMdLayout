{
  "presets": ["env", "react", "stage-0"],
  "sourceMaps": true,
  "plugins": [
    [
        "import",
        {
            "libraryName": "material-ui",
            "libraryDirectory": "components",
            "camel2DashComponentName": false,
        }
    ],
    "react-hot-loader/babel",
    "transform-decorators-legacy",
    ["transform-class-properties", { "loose": true }]
  ]
 }
